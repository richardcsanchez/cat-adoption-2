<h1 id="agency_name"><%= @agency.name %></h1>
<h4 class="agency_info"><%= @agency.email%></h4>
<h4 class="agency_info"><%= number_to_phone(@agency.phone_number)%></h4>

<div class="agency_address">
  <p>
    <%=@agency.street_1%><br>
    <%=@agency.street_2%>
    <%=@agency.city%>
    <%=@agency.state%>
    <%=@agency.zip_code%>
  </p>
  </div>
<%= link_to "Cats at this Agency", agency_cats_path(@agency.id)%>

<%if current_user.admin?%>
<%= link_to "Add Cat", new_agency_cat_path(@agency.id)%>
<%= link_to 'Edit', edit_agency_path(@agency)%>
<%end%>
<br>
<br>
